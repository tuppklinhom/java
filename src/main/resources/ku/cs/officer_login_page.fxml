            <Font name="Cambria" size="18.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
         <styleClass>
            <String fx:value="login_button" />
            <String fx:value="shadow" />
         </styleClass>
      </Button>

      <TextField fx:id="officerUsernameTextField" layoutX="806.0" layoutY="378.0" prefHeight="38.0" prefWidth="226.0" promptText="username" style="-fx-background-radius: 20;">
         <font>
            <Font size="16.0" />
         </font></TextField>
      <PasswordField fx:id="officerPasswordTextField" layoutX="808.0" layoutY="471.0" prefHeight="38.0" prefWidth="226.0" promptText="password" style="-fx-background-radius: 20;">
