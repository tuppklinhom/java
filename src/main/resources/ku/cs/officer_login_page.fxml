            <Image url="@kuuni/ku_logo.jpg" />
         </image>
      </ImageView>
      <Label layoutX="806.0" layoutY="334.0" text="Username">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="808.0" layoutY="433.0" text="Password">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Label>
      <Button layoutX="1060.0" layoutY="46.0" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="38.0" prefWidth="82.0" styleClass="logout_button" stylesheets="@style_page/stylee.css" text="Back" textFill="#fffefe">
         <effect>
            <DropShadow />
         </effect>
         <font>
            <Font name="Cambria" size="18.0" />
         </font>
      </Button>
      <ImageView fitHeight="899.0" fitWidth="599.0" layoutY="-7.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@kuuni/Ku_help.png" />
